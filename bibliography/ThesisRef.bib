journal = {Journal of Crystal Growth},
keywords = {A1. Substrates,A3. Metalorganic vapor phase epitaxy,B1. Antimonides,B1. Nitrides,B2. Semiconducting III V materials,B3. Solar cells},
pages = {87--91},
title = {{Impact of growth temperature and substrate orientation on dilute-nitride-antimonide materials grown by MOVPE for multi-junction solar cell application}},
