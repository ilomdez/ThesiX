pages = {03010},
title = {{The Next Generation of Germanium Substrates: Expoger TM}},
url = {https://www.e3s-conferences.org/articles/e3sconf/abs/2017/04/e3sconf{\_}espc2017{\_}03010/e3sconf{\_}espc2017{\_}03010.html},
volume = {16},
