pages = {520--527},
shorttitle = {Photovoltaics, IEEE Journal of},
title = {{Luminescent Coupling in GaAs/GaInNAsSb Multijunction Solar Cells}},
url = {https://ieeexplore.ieee.org/document/6301669?arnumber=6301669{\&}tag=1},
volume = {3},
year = {2013}
}
@article{Isoaho2019,
