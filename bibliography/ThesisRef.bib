keywords = {CPV,LCOE,location influence,real application,reliability},
number = {11},
pages = {905--918},
title = {{Reliability of commercial triple junction concentrator solar cells under real climatic conditions and its influence on electricity cost}},
url = {https://onlinelibrary.wiley.com/doi/full/10.1002/pip.2902},
