keywords = {A1. Growth models,A3. Molecular beam epitaxy,B1. Gallium compounds,B2. Semiconducting quaternary alloys,B3. Solar cells},
number = {1-4},
pages = {249--253},
title = {{A possibility of In-N fragments incorporation in InGaAsN MBE growth}},
url = {https://www.sciencedirect.com/science/article/pii/S0022024804020548},
