issn = {0038092X},
journal = {Solar Energy},
keywords = {Deep Level Transient Fourier Spectroscopy,InGaAsN/GaAs,Semiconductor defect,Tandem solar cell},
pages = {587--590},
publisher = {Elsevier Ltd},
title = {{Defect distribution in InGaAsN/GaAs multilayer solar cells}},
url = {http://dx.doi.org/10.1016/j.solener.2016.03.057},
volume = {132},
year = {2016}
}
