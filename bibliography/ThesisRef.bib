keywords = {Annealing,Dilute Nitride Semiconductor,Multijunction Solar Cell},
number = {1},
pages = {1--27},
title = {{Characterization of 1.0 eV GaInNAsSb Solar Cells for Miltijunction Applications and the Effect of Annealing}},
url = {https://www.eupvsec-proceedings.com/proceedings?paper=34293},
volume = {2},
