keywords = {61.72.−y,68.37.Lp,71.55.Eq,81.05.Ea,81.15.Gh,A1. Defects,A3. Metalorganic vapor phase epitaxy,B2. Semiconducting III/V materials,B2. Semiconducting iii/v materials,B3. Laser diodes,B3. Solar cells},
month = {dec},
number = {1-4},
pages = {739--747},
title = {{Specific structural and compositional properties of (GaIn)(NAs) and their influence on optoelectronic device performance}},
