@article{Kudrawiec2012,
abstract = {The influence of non-radiative recombination on the photoluminescence$\backslash$ndecay time (tau(PL)) has been studied for GaInNAs/GaAs quantum wells$\backslash$nwith Ga- and In-rich environments of N atoms. At low temperatures, this$\backslash$ninfluence is suppressed, due to the carrier localization phenomenon,$\backslash$nwhich leads to a spectral dispersion of tau(PL). For investigated$\backslash$nsamples, this dispersion has been found to be in the range of similar to$\backslash$n0.2-2.0 ns. With the temperature increase, the free exciton emission$\backslash$nstarts to dominate instead of the localized exciton emission and the$\backslash$ndispersion of tau(PL) disappears. The dynamic of free exciton$\backslash$nrecombination is strongly influenced by the non-radiative recombination,$\backslash$nwhich varies between samples, due to different concentration of$\backslash$nnon-radiative centers. The study of influence of non-radiative$\backslash$nrecombination on tau(PL) has been performed at 180 K, since this$\backslash$ntemperature is high enough to eliminate the localized emission and$\backslash$nactivate non-radiative recombination and low enough to observe excitonic$\backslash$nemission without strong contribution of free carrier recombination when$\backslash$nthe sample is excited with low power. It was observed that, for as-grown$\backslash$nsamples, the tau(PL) increases from 0.14 to 0.25 ns with the change in$\backslash$nAs/III beam equivalent pressure ratio from 3.8 to 12.1 (in this case, it$\backslash$ncorresponds to the change in nitrogen nearest-neighbor environment from$\backslash$nGa- to In-rich), whereas, after annealing (i.e., also the change from$\backslash$nGa-rich to In-rich environment of N atoms), this time increases 2-4$\backslash$ntimes, depending on the As/III ratio. It has been concluded that the$\backslash$ntau(PL) is influenced by point defects rather than the nitrogen$\backslash$nnearest-neighbor environment, but their concentration is correlated with$\backslash$nthe type of nitrogen environment. (C) 2012 American Institute of$\backslash$nPhysics. {\{}[{\}}http://dx.doi.org/10.1063/1.3695457]},
author = {Kudrawiec, R. and Syperek, M. and Latkowska, M. and Misiewicz, J. and Korpij{\"{a}}rvi, V. M. and Laukkanen, P. and Pakarinen, J. and Dumitrescu, M. and Guina, M. and Pessa, M.},
doi = {10.1063/1.3695457},
file = {:C$\backslash$:/Users/ilh/Documents/Mendeley Desktop//Kudrawiec et al. - 2012 - Influence of non-radiative recombination on photoluminescence decay time in GaInNAs quantum wells with Ga- and.pdf:pdf},
issn = {00218979},
journal = {Journal of Applied Physics},
number = {6},
pages = {1--6},
title = {{Influence of non-radiative recombination on photoluminescence decay time in GaInNAs quantum wells with Ga- and In-rich environments of nitrogen atoms}},
url = {https://aip.scitation.org/doi/10.1063/1.3695457},
volume = {111},
year = {2012}
}
