pages = {104510},
title = {{Electroluminescence analysis of high efficiency Cu(In,Ga)Se[sub 2] solar cells}},
url = {http://scitation.aip.org/content/aip/journal/jap/102/10/10.1063/1.2817959},
volume = {102},
year = {2007}
}
