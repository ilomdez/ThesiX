number = {3},
pages = {467--475},
title = {{Recombination processes in N-containing III-V ternary alloys}},
url = {https://www.sciencedirect.com/science/article/pii/S0038110102003908?via{\%}3Dihub},
volume = {47},
year = {2003}
}
